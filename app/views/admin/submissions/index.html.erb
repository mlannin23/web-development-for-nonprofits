<div class="breadcrumbs">
  <div class="container-fluid">
    <ol class="breadcrumb">
      <li><%= link_to "Admin", admin_path %></li>
      <li><%= link_to "Assignments", admin_assignments_path %></li>
      <li><%= link_to "Assignment", admin_assignment_path(@assignment) %></li>
      <li class="active">Submissions</li>
    </ol>
  </div>
</div>

<div class="section">
  <div class="container-fluid">
    <h2>Submissions</h2>

    <table class="table">
      <tr>
        <th>User</th>
        <th>File</th>
      </tr>
     
      <% @submissions.each do |submission| %>
        <tr>
          <td><%= submission.user.email %></td>
          <td><%= submission.file.url %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>