<div class="breadcrumbs">
  <div class="container-fluid">
    <ol class="breadcrumb">
      <li><%= link_to "Admin", admin_path %></li>
      <li><%= link_to "Assignments", admin_assignments_path %></li>
      <li class="active">Assignment</li>
    </ol>
  </div>
</div>

<div class="section">
  <div class="container-fluid">
    <h2>Assignment Details</h2>

    <p>
      <strong>Season:</strong>
      <%= @assignment.title %>
    </p>
     
    <p>
      <strong>Year:</strong>
      <%= link_to @assignment.url, @assignment.url, target: "_blank" %>
    </p>

    <%= link_to 'Edit', edit_admin_assignment_path(@assignment), { class: "btn btn-default" } %>
  </div>
</div>